extends base.pug

block scripts 
    script(src='/static/index.js')
    script(src='/static')


block style 
    style 
        include ../static/style.css
        include ../static/stylec.css



block content
  h1 Contact Us
  div.container
    form(action="/contact", method="post" Class="form") 
      input(type="text", name='name',Class='myinput', placeholder='Enter Your Name')
      input(type="phone", name='phone',Class='myinput', placeholder='Enter Your Number')
      input(type="email", name='email',Class='myinput', placeholder='Enter Your Email')
      input(type="text", name='address',Class='myinput', placeholder='Enter Your Address')
      input(type="text", name='desc',Class='myinput', placeholder='Enter Your Desc')
      //- button(Class='btn')
      //-   |submit 
      button(Class='btn')
        |submit 